{% extends 'base_sin_menu.html.twig' %}
{% block content %}
    <div class="row">
        <div class="col-lg-8">
            <div class="row">
                <div class="col-lg-12">
                    <small style="font-size: 20px">{{ crearTitulo(arConfiguracionEntidad,1) }}</small>
                </div>
            </div>
        </div>
        <table class="table table-striped table-bordered table-hover tabla-datos">
            <thead>
            <tr>
                {% if arRegistros.getTotalItemCount > 0 %}
                    {% for llave,dato in arRegistros[0] %}
                        <th>{{ llave }}</th>
                    {% endfor %}
                {% endif %}
            </tr>
            </thead>
            <tbody>
            {% for llave,datos in arRegistros %}
                <tr>
                    {% for key,dato in datos %}
                           <td> {{ dato }} </td>
                    {% endfor %}
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
