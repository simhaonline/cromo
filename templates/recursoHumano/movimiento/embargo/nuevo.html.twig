{% extends 'base_recursoHumano.html.twig' %}
{% block content %}
    {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}
    <br>
    <div class="panel panel-default">
        <div class="panel-heading">Informaci√≥n general</div>
        <div class="panel-body">
            <div class="form-group form-group-sm">
                <label for="form_fecha" class="col-sm-2 control-label">Fecha:</label>
                <div class="col-sm-4">
                    {{ form_widget(form.fecha) }}
                    {{ form_errors(form.fecha) }}
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <a class="btn btn-default btn-sm"
           href="{{ path('recursohumano_movimiento_embargo_embargo_lista') }}">Cancelar</a>
        {{ form_widget(form.guardar, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}
    </div>
    {{ form_end(form) }}
{% endblock %}
