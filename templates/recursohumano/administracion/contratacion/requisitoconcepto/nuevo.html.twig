{% extends 'base.html.twig' %}
{% block content %}
    {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}
    <div class="panel panel-default">
        <div class="panel-heading">Informaci√≥n general</div>
        <div class="panel-body">
            <div class="form-group form-group-sm">
                <label for="form_pedidoTipoRel" class="col-sm-2 control-label">Nombre:</label>
                <div class="col-sm-4">
                    {{ form_widget(form.nombre, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.nombre) }}
                </div>
                <label for="form_sectorRel" class="col-sm-2 control-label">General:</label>
                <div class="col-sm-4">
                    {{ form_widget(form.general, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.general) }}
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <a class="btn btn-default btn-sm" href="{{ path('recursohumano_administracion_contratacion_requisitoconcepto_lista') }}">Cancelar</a>
        {{ form_widget(form.guardar, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}
    </div>
    {{ form_end(form) }}
{% endblock %}
