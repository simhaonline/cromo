{% extends 'base.html.twig' %}
{% block content %}
    {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}
    <div class="btn-toolbar" style="float:left">
        <div class="btn-group">
            <a class="btn btn-default btn-sm"
               href="{{ path('recursohumano_movimiento_seguridadsocial_aporte_lista') }}">Volver</a> &nbsp;&nbsp;
        </div>
    </div>
    <br><br>
    <table width="100%" class="table table-condensed table-bordered tabla-informacion">
        <tbody>
        <tr>
            <td class="titulo">ID:</td>
            <td class="td-in-well">{{ arAporte.codigoAportePk }}</td>
            <td class="titulo">AÃ‘O:</td>
            <td class="td-in-well">{{ arAporte.anio }}</td>
            <td class="titulo">MES:</td>
            <td class="numero">{{ arAporte.mes}}</td>
        </tr>
        <tr>
            <td class="titulo">SUCURSAL:</td>
            <td class="td-in-well">{{ arAporte.sucursalRel.nombre }}</td>
            <td class="titulo"></td>
            <td class="td-in-well"></td>
            <td class="titulo"></td>
            <td class="numero"></td>
        </tr>
    </table>
    <ul class="nav nav-tabs">
        <li class="active"><a href="#detalle" data-toggle="tab">Detalle</a></li>
        <li><a href="#contrato" data-toggle="tab">Contrato</a></li>
    </ul>
    <div class="tab-content">
        <div class="tab-pane active" id="detalle">
            <div id="detalle" style="width: 100%"><br>
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover tabla-datos">
                        <thead>
                        <tr>
                            <th>ID</th>
                            <th>CON</th>
                            <th>DOC</th>
                            <th>NOMBRE</th>
                            <th></th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for arAporteDetalle in arAporteDetalles %}
                            <tr>
                                <td>{{ arAporteContrato.codigoAporteContratoPk }}</td>
                                <td>{{ arAporteContrato.codigoContratoFk }}</td>
                                <td>{{ arAporteContrato.empleadoNumeroIdentificacion }}</td>
                                <td>{{ arAporteContrato.empleadoNombreCorto }}</td>
                                <td>{{ arAporteContrato.dias }}</td>
                                <td>{{ arAporteContrato.horas }}</td>
                                <td class="numero">{{ arAporteContrato.vrSalario  | number_format(0, '.', ',')}}</td>
                                <td>{{ arAporteContrato.Ibc }}</td>
                                <td>{{ arAporteContrato.IbcCajaVacaciones }}</td>
                                <td>{{ arAporteContrato.vrVacaciones }}</td>
                                <td>{{ arAporteContrato.ingreso }}</td>
                                <td>{{ arAporteContrato.retiro }}</td>
                                <td>{{ arAporteContrato.salarioIntegral }}</td>
                                <td>{{ arAporteContrato.diasLicencia }}</td>
                                <td>{{ arAporteContrato.diasIncapacidadGeneral }}</td>
                                <td>{{ arAporteContrato.diasLicenciaMaternidad }}</td>
                                <td>{{ arAporteContrato.diasIncapacidadLaboral }}</td>
                                <td>{{ arAporteContrato.diasVacaciones }}</td>
                                <td>{{ arAporteContrato.tarifaPension }}</td>
                                <td>{{ arAporteContrato.tarifaSalud }}</td>
                                <td>{{ arAporteContrato.tarifaRiesgos }}</td>
                                <td>{{ arAporteContrato.tarifaCaja }}</td>
                                <td>{{ arAporteContrato.codigoEntidadPensionPertenece }}</td>
                                <td>{{ arAporteContrato.codigoEntidadSaludPertenece }}</td>
                                <td>{{ arAporteContrato.codigoEntidadCajaPertenece }}</td>
                                <td>{{ arAporteContrato.incapacidadGeneral?"SI":"NO" }}</td>
                                <td>{{ arAporteContrato.incapacidadLaboral?"SI":"NO"  }}</td>
                                <td>{{ arAporteContrato.licencia?"SI":"NO"  }}</td>
                                <td>{{ arAporteContrato.licenciaMaternidad?"SI":"NO"     }}</td>
                                <td>{{ arAporteContrato.licenciaRemunerada?"SI":"NO"  }}</td>
                                <td>{{ arAporteContrato.vacaciones?"SI":"NO"  }}</td>
                                <td>{{ arAporteContrato.fechaDesde | date('Y-m-d') }}</td>
                                <td>{{ arAporteContrato.fechaHasta | date('Y-m-d') }}</td>
                                <td>{{ arAporteContrato.fechaRetiro | date('Y-m-d') }}</td>
                                <td>{{ arAporteContrato.fechaIngreso | date('Y-m-d') }}</td>
                                <td>{{ arAporteContrato.variacionTransitoriaSalario }}</td>
                                <td>{{ arAporteContrato.codigoEntidadSaludTraslada }}</td>
                                <td>{{ arAporteContrato.trasladoAOtraEps?"SI":"NO" }}</td>
                                <td>{{ arAporteContrato.trasladoAOtraPension?"SI":"NO" }}</td>
                                <td>{{ arAporteContrato.trasladoDesdeOtraEps?"SI":"NO" }}</td>
                                <td>{{ arAporteContrato.trasladoDesdeOtraPension?"SI":"NO" }}</td>
                                </td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
                <div class="separador-1"></div>
                <div class="btn-toolbar" style="float: right">
                    <div class="btn-group">
                        {{ form_widget(form.btnExcelDetalle) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="contrato">
            <div id="contrato" style="width: 100%"><br>
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover tabla-datos">
                        <thead>
                        <tr>
                            <th>ID</th>
                            <th>DOC</th>
                            <th>NOMBRE</th>
                            <th>CON</th>
                            <th>D</th>
                            <th>SALARIO</th>
                            <th>IBC</th>
                            <th></th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for arAporteContrato in arAporteContratos %}
                            <tr>
                                <td>{{ arAporteContrato.codigoAporteContratoPk }}</td>
                                <td>{{ arAporteContrato.empleadoNumeroIdentificacion }}</td>
                                <td>{{ arAporteContrato.empleadoNombreCorto }}</td>
                                <td>{{ arAporteContrato.codigoContratoFk }}</td>
                                <td>{{ arAporteContrato.dias }}</td>
                                <td class="numero">{{ arAporteContrato.vrSalario  | number_format(0, '.', ',')}}</td>
                                <td class="numero">{{ arAporteContrato.ibc  | number_format(0, '.', ',')}}</td>
                                <td style="text-align: center;"><a
                                            href="javascript:abrirVentana3('{{ path('recursohumano_movimiento_seguridadsocial_soporte_aporte_detalle',{ 'id': arAporteContrato.codigoAporteContratoPk }) }}','resumen',600,1000)"><i
                                                class="fa fa-table" style="color: black;font-size: large;"></i></a></td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
                <div class="separador-1"></div>
                <div class="btn-toolbar" style="float: right">
                    <div class="btn-group">
                        {{ form_widget(form.btnExcelContrato) }}
                    </div>
                </div>
            </div>
        </div>

    </div>
    {{ form_end(form) }}

{% endblock %}
