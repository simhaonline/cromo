            <div class="form-group form-group-sm">
                <label for="identificacionRel" class="col-sm-2 control-label">Ingreso:</label>
                <div class="col-sm-4">
                    {{ form_widget(form.ingreso) }}
                </div>
                <label for="form_mes" class="col-sm-2 control-label">Retiro:</label>
                <div class="col-sm-4">
                    {{ form_widget(form.retiro, { 'attr': {'class': 'form-control','autofocus':'autofocus'} }) }}
