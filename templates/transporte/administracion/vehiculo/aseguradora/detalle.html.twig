{% extends 'base.html.twig' %}
{% block content %}
    <div class="btn-toolbar" style="float:left">
        <div class="btn-group">
            <a class="btn btn-default btn-sm"
               href="{{ path('transporte_administracion_vehiculo_aseguradora_lista') }}">Volver</a> &nbsp;&nbsp;
        </div>
    </div>
    <br><br>
    <table width="100%" class="table table-condensed table-bordered tabla-informacion">
        <tbody>
            <tr>
                <td class="titulo">ID:</td>
                <td class="td-in-well">{{ arRegistro.codigoAseguradoraPk }}</td>
                <td class="titulo">INGRESO:</td>
                <td class="td-in-well">{{ arRegistro.nombre }}</td>
            </tr>
            <tr>
                <td class="titulo">NUMERO IDENTIFICACIÓN:</td>
                <td class="td-in-well">{{ arRegistro.numeroIdentificacion }}</td>
                <td class="titulo">DIGITO VERIFICACIÓN:</td>
                <td class="td-in-well">{{ arRegistro.digitoVerificacion }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
